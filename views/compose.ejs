<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle" class="form-label">Title</label>
    <input class="form-control" id="postTitle" type="text" name="postTitle" autocomplete="off"/>
    <label for="postBody" class="form-label">Post</label>
    <textarea
      class="form-control"
      name="postBody"
      id="postBody"
      rows="5"
      cols="30"
    ></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
